{
  "cli": {
    "version": ">= 16.20.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "distribution": "internal"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    },
    "ios-preview": {
      "autoIncrement": true,
      "ios": {
        "simulator": true,
        "distribution": "internal"
      }
    },
    "ios-production": {
      "autoIncrement": true,
      "ios": {
        "distribution": "store"
      }
    },
    "ios-device": {
      "ios": {
        "simulator": false,
        "distribution": "internal"
      }
    },
    "ios-testflight": {
      "autoIncrement": true,
      "ios": {
        "simulator": false,
        "distribution": "store"
      }
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
